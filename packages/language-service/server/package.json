{
    "name": "@master/css-language-server",
    "scripts": {
        "build": "aron pack --platform node --shakeable-format",
        "dev": "npm run build -- --watch",
        "type-check": "tsc --noEmit",
        "lint": "eslint src"
    },
    "description": "VSCode part of a language server",
    "author": "Aoyue Design LLC.",
    "license": "MIT",
    "engines": {
        "node": "*"
    },
    "devDependencies": {
        "@master/css": "",
        "@master/css-compiler": "",
        "file-uri-to-path": "^2.0.0",
        "js-beautify": "^1.14.7",
        "minimatch": "^5.1.2",
        "pascal-case": "^3.1.2",
        "vscode-css-languageservice": "^6.2.3",
        "vscode-languageserver": "^7.0.0",
        "vscode-languageserver-textdocument": "^1.0.4"
    },
    "main": "./dist/server.js",
    "jsnext:main": "./dist/server.mjs",
    "esnext": "./dist/server.mjs",
    "module": "./dist/server.mjs",
    "types": "./dist/server.d.ts",
    "exports": {
        ".": {
            "require": "./dist/server.js",
            "import": "./dist/server.mjs",
            "types": "./dist/server.d.ts"
        }
    },
    "files": [
        "dist"
    ],
    "publishConfig": {
        "access": "public"
    }
}